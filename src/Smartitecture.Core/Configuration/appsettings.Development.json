{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft": "Warning"
        }
    },
    "AllowedHosts": "*",
    "Security": {
        "JwtSecret": "YourDevelopmentSecretKey",
        "Issuer": "smartitecture-dev",
        "Audience": "smartitecture-dev",
        "TokenExpirationMinutes": 60
    },
    "OpenAI": {
        "ApiKey": "YourOpenAIDevKey",
        "ApiEndpoint": "https://api.openai.com/v1",
        "MaxRetries": 3,
        "RequestTimeoutSeconds": 30
    },
    "Database": {
        "ConnectionString": "Server=localhost;Database=SmartitectureDev;Trusted_Connection=True;MultipleActiveResultSets=true"
    },
    "ExternalServices": {
        "TimeoutSeconds": 30,
        "RetryAttempts": 3
    }
}
